{
  "extends": "airbnb-base/legacy",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "arrowFunctions": true,
      "classes": true,
      "modules": true,
      "defaultParams": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "func-names": 0,
    "object-shorthand": 0,
    "comma-dangle": [2, "never"],
    "prefer-template": 0,
    "consistent-return": 0,
    "block-scoped-var": 0,
    "no-underscore-dangle": [2, {
      "allow": ["__INITIAL_STATE__"]
    }],
    "linebreak-style": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": [2, {
      "props": false
    }],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "no-did-mount-set-state": 0,
    "react/forbid-prop-types": [2, {
      "forbid": ["array"]
    }],
    "no-unused-vars": 0,
    "global-require": 0,
    "radix": 0,
    "no-lonely-if": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "no-bitwise": [2, { "allow": ["~"] }],
    "no-restricted-syntax": 0,
    "no-prototype-builtins": 0
  }
}
